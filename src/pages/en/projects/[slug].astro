---
import BaseLayout from "../../../layouts/BaseLayout.astro";
import { getEntryBySlug } from "astro:content";
const { slug } = Astro.params;
const project = await getEntryBySlug("projects_en", slug);
const { Content } = await project.render();
---
<BaseLayout title={project.data.title} lang="en">
  <p><strong>{project.data.role}</strong> Â· {project.data.year}</p>
  <p>{project.data.tags.join(", ")}</p>
  <Content />
</BaseLayout>
